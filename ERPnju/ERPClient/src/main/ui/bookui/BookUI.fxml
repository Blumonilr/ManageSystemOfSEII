<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="724.0" prefWidth="998.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.ui.bookui.BookUIController">
   <children>
      <Label alignment="TOP_LEFT" layoutX="32.0" layoutY="3.0" prefHeight="35.0" prefWidth="106.0" text="期初建账" textFill="#053896">
         <font>
            <Font name="Microsoft YaHei" size="25.0" />
         </font>
      </Label>
      <JFXTabPane layoutY="44.0" prefHeight="618.0" prefWidth="998.0" stylesheets="@../mainui/tabPane.css">
         <tabs>
            <Tab text="商品分类">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="998.0">
                     <children>
                        <ScrollPane fx:id="classPane" prefHeight="585.0" prefWidth="998.0" />
                     </children></AnchorPane>
              </content>
            </Tab>
            <Tab text="商品信息">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ScrollPane fx:id="goodsPane" prefHeight="585.0" prefWidth="998.0" />
                     </children></AnchorPane>
              </content>
            </Tab>
            <Tab text="客户信息">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ScrollPane fx:id="clientPane" prefHeight="585.0" prefWidth="998.0" />
                     </children></AnchorPane>
              </content>
            </Tab>
            <Tab text="银行账户信息">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ScrollPane fx:id="accountsPane" prefHeight="585.0" prefWidth="998.0" />
                     </children></AnchorPane>
              </content>
            </Tab>
         </tabs>
      </JFXTabPane>
      <AnchorPane layoutY="661.0" prefHeight="63.0" prefWidth="998.0">
         <children>
            <Button fx:id="submit" layoutX="113.0" layoutY="16.0" mnemonicParsing="false" onAction="#onSubmit" prefHeight="31.0" prefWidth="117.0" text="确认提交">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="save" layoutX="428.0" layoutY="16.0" mnemonicParsing="false" onAction="#onSave" prefHeight="31.0" prefWidth="117.0" text="保存">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="cancel" layoutX="718.0" layoutY="16.0" mnemonicParsing="false" onAction="#onCancel" prefHeight="31.0" prefWidth="117.0" text="取消">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
